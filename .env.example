# Environment Variables Template
# Copy this file to .env and fill in your values

# Telegram Bot
BOT_TOKEN=your_telegram_bot_token_from_botfather

# Slack Bot (Socket Mode)
SLACK_BOT_TOKEN=xoxb-your-token
SLACK_SIGNING_SECRET=your-signing-secret
SLACK_APP_TOKEN=xapp-your-app-token

# Azure Speech Services (for Voice Biometrics)
AZURE_SPEECH_KEY=your_azure_speech_key
AZURE_SPEECH_REGION=your_azure_region
AZURE_SPEECH_ENDPOINT=https://your_region.api.cognitive.microsoft.com

# Gemini AI (get from aistudio.google.com)
GEMINI_API_KEY=your_gemini_api_key

# Optional: Multiple Gemini keys for rotation (anti-rate-limiting)
GEMINI_API_KEY_1=key1
GEMINI_API_KEY_2=key2
GEMINI_API_KEY_3=key3
GEMINI_API_KEY_4=key4
GEMINI_API_KEY_5=key5
GEMINI_API_KEY_6=key6

# Wallet (for signing mandates - use a testnet key!)
PRIVATE_KEY=0x_your_private_key_here
